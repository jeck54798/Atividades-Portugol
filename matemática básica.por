programa {
  funcao inicio() {
    
    inteiro n1 , n2

    escreva("Qual é o primeiro valor? ")
    leia(n1)
    escreva("E qual é o segundo valor? ")
    leia(n2)

    escreva("O valor das 4 operações matemáticas são: \n")
    escreva(n1 + n2)
    escreva(" para adição\n")
    escreva(n1 - n2)
    escreva(" para subtração\n")
    escreva(n1 * n2)
    escreva(" para multiplicação\n")
    escreva(n1 / n2)
    escreva(" para divisão")
  }
}
