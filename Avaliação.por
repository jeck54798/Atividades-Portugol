programa {
  funcao inicio() {
    inteiro resultado

    escreva("10 / 5 = ")
    leia(resultado)

    se(resultado == 2){
      escreva("resposta certa")
    }senao escreva("resposta errada")

    escreva("4² + 1 = ")
    leia(resultado)

    se(resultado == 17){
      escreva("resposta certa")
    }senao escreva("resposta errada")

    escreva("41 * 9 = ")
    leia(resultado)

    se(resultado == 378){
      escreva("resposta certa")
    }senao escreva("resposta errada")
    
  }
}
